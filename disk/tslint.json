{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "max-classes-per-file": false,
    "no-console": false,
    "no-empty": false,
    "prefer-for-of": false,
    "only-arrow-functions": false,
    "variable-name": false,
    "trailing-comma": false,
    "interface-name": false
  }
}
